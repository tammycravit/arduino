void scrollText(String text)
{
  display.setTextSize(1);
  display.setTextColor(WHITE);
  display.setTextWrap(false);
  int x = display.width();
  int minX = -6 * text.length(); // 6 = 6 pixels/character * text size 1 
  for(int y = 0; y < 6 * text.length() + 6; y++) { //Full text length plus one additional character
  display.fillRect(0,4,128,11,BLACK);
  display.setCursor(x, 4);
  display.print(text);
  display.display();
  if(--x < minX) x = display.width();
  }
 delay(500);
 display.fillRect(0,4,128,11,BLACK);
 display.setCursor(34,4);
 display.print("Pusheen!!!");
 display.display();
}
